
{
  "entities": {
    "Note": {
      "title": "Note",
      "description": "A simple note with content.",
      "type": "object",
      "properties": {
        "content": {
          "type": "string",
          "description": "The content of the note."
        },
        "createdAt": {
          "type": "string",
          "format": "date-time",
          "description": "The timestamp when the note was created."
        }
      },
      "required": ["content", "createdAt"]
    }
  },
  "auth": {
    "providers": []
  },
  "firestore": {
    "/notes/{noteId}": {
      "schema": { "$ref": "#/entities/Note" },
      "description": "Stores user notes."
    }
  }
}
